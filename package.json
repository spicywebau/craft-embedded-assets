  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "standard": "^16.0.4",
    "style-loader": "^3.3.1",
